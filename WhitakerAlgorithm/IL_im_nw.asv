function [ ] = IL_im_nw(i)
%matlabpool open
[inv_factors,x_mat,comb] = InvGen(4,6.5);
%for i=1:15,
	Corrupted_Data = Image_Encoder(4,2,6.5,'images/IL.bmp',i,1);
	Corrected_Whitaker_Data = Run_MyAlgorithm(4,6,Corrupted_Data,inv_factors,comb,x_mat);
	Image_Reconstruction_nw(4,6.5,'images/IL.bmp',Corrected_Whitaker_Data,i)
%end;
matlabpool close

